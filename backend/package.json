{
  "name": "@healthflow/backend",
  "version": "1.0.0",
  "description": "HealthFlow Digital Prescription Portals - Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testMatch='**/*.integration.test.ts'",
    "test:e2e": "jest --testMatch='**/*.e2e.test.ts'",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "migrate": "knex migrate:latest --knexfile src/shared/database/knexfile.ts",
    "migrate:up": "knex migrate:latest --knexfile src/shared/database/knexfile.ts",
    "migrate:down": "knex migrate:rollback --knexfile src/shared/database/knexfile.ts",
    "migrate:make": "knex migrate:make --knexfile src/shared/database/knexfile.ts",
    "migrate:status": "knex migrate:status --knexfile src/shared/database/knexfile.ts",
    "migrate:list": "knex migrate:list --knexfile src/shared/database/knexfile.ts",
    "seed": "knex seed:run --knexfile src/shared/database/knexfile.ts",
    "seed:make": "knex seed:make --knexfile src/shared/database/knexfile.ts"
  },
  "keywords": [
    "healthflow",
    "prescription",
    "eda",
    "healthcare"
  ],
  "author": "HealthFlow Team",
  "license": "UNLICENSED",
  "dependencies": {
    "@elastic/elasticsearch": "^8.10.0",
    "axios": "^1.5.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^2.5.1",
    "node-cron": "^4.2.1",
    "papaparse": "^5.4.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "winston": "^3.10.0",
    "xlsx": "^0.18.5",
    "xmlbuilder2": "^3.1.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.8.0",
    "@types/papaparse": "^5.3.8",
    "@types/pg": "^8.10.3",
    "@types/supertest": "^2.0.13",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
